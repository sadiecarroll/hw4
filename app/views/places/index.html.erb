<h1>Places</h1>

    

<% if User.find_by({ "id" => session["user_id"] }) != nil %>
<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
  <% end %>
</ul>

<p>
  <a href="/places/new">New place</a>
</p>

<% else %>
  <p>You must first <%= link_to "login", login_path %> to see Places.</p>
<% end %>

