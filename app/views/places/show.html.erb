<h1><%= @place["name"] %></h1>

<h2>My Entries</h2>
<%  %>
<ul>

<div class="container text-center">
  <div class="row">
  
<% for entry in @entries %>

  <div class="col-md-4">
    
     <h3> <%= entry["title"] %> </h3>
     <h6> <%= entry["occurred_on"] %> </h6>
      <%= entry["description"] %>
        </div>

  <% end %>
</ul>

<p>
  <a href="/entries/new?place_id=<%= @place["id"] %>">New Entry</a>
</p>

<p>
  <a href="/places">Back to All Places</a>
</p>